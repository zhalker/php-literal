    "keywords": [
        "php",
        "template",
        "literal",
        "backtick",
        "template-literals"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "zhalker",
            "email": "willardh.knox@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "zhalker/dumb-artifact-for-multi-level-contextual-parsing-in-php": "^1.1"
    },
    "autoload": {
        "files": [
            "src/autoload.php"
        ],
        "psr-4": {
            "PHPLiteral\\": "src/"
        }
    },
    "minimum-stability": "stable"
}